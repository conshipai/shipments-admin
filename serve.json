{
  "headers": [
    {
      "source": "remoteEntry.js",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "!**/*.{js,css,html,json,map,jpg,png,gif,svg,ico}",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}
