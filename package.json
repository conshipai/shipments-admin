{
  "name": "shipments-admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "serve -s dist -l 3002 -c ../serve.json",
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production && node verify-build.js",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "verify": "node verify-build.js",
    "serve:local": "serve -s dist -l 3002 -c ../serve.json",
    "docker:build": "docker build -t shipments-admin .",
    "docker:run": "docker run -p 3002:3002 shipments-admin",
    "test:federation": "npx http-server dist -p 3002 --cors"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "axios": "^1.6.2",
    "lucide-react": "^0.294.0",
    "date-fns": "^2.30.0",
    "serve": "^14.2.0"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "postcss-loader": "^7.3.3",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "http-server": "^14.1.1"
  }
}
